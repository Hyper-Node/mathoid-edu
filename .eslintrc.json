{
	"root": true,
	"extends": [
		"wikimedia/server",
		"wikimedia/jsdoc"
	],
	"rules": {
		"camelcase": [
			"error",
			{
				"properties": "never"
			}
		],
		"no-unused-vars": [
			"error",
			{
				"args": "none"
			}
		],
		"space-in-parens": [
			"error",
			"never"
		],
		"computed-property-spacing": "off",
		"no-multi-spaces": "off"
	}
}
